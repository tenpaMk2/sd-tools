{
  "name": "@tenpamk2/sd-tag-defines",
  "version": "1.0.0",
  "author": "tenpaMk2 <wavyroom@gmail.com> (https://github.com/tenpaMk2)",
  "description": "ðŸ”– Tag defines for my Stable Diffusion tools ðŸ”–",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist",
    "readme-ja.md"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build:base": "npx esbuild --bundle --platform=neutral --out-extension:.js=.mjs src/index.mts --outdir=dist",
    "build:esbuild": "npm run build:base -- --minify",
    "build:declaration": "tsc",
    "build": "run-p build:esbuild build:declaration",
    "watch:esbuild": "npm run build:base -- --sourcemap --keep-names --watch",
    "watch:declaration": "tsc --watch",
    "watch": "run-p watch:esbuild watch:declaration",
    "clean": "rimraf dist",
    "test": "vitest"
  },
  "license": "MIT",
  "private": true,
  "keywords": [
    "ai",
    "ai-art",
    "stable-diffusion"
  ],
  "devDependencies": {
    "esbuild": "^0.21.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
