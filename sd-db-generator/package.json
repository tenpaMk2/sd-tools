{
  "name": "@tenpamk2/sd-db-generator",
  "version": "1.0.0",
  "author": "tenpaMk2 <wavyroom@gmail.com> (https://github.com/tenpaMk2)",
  "description": "🗃️ Database generator for `@tenpamk2/sd-random-generator` 🗄️",
  "exports": {
    "node": "./dist/index.mjs",
    "default": "./dist/index.mjs"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "readme-ja.md"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build:base": "npx esbuild --bundle --platform=neutral --out-extension:.js=.mjs src/index.mts --outdir=dist",
    "build:esbuild": "npm run build:base -- --minify",
    "build:declaration": "tsc",
    "build:db": "npx esbuild --bundle --platform=node --out-extension:.js=.cjs src/index-db.mts --outdir=dist-db",
    "build": "run-p build:esbuild build:declaration build:db",
    "watch:esbuild": "npm run build:base -- --sourcemap --keep-names --watch",
    "watch:declaration": "tsc --watch",
    "watch:db": "npm run build:db -- --sourcemap --keep-names --watch",
    "watch": "run-p watch:esbuild watch:declaration watch:db",
    "clean": "rimraf dist dist-db",
    "exe": "node dist-db/index-db.cjs",
    "test": "vitest"
  },
  "license": "MIT",
  "private": true,
  "keywords": [
    "ai",
    "ai-art",
    "stable-diffusion"
  ],
  "dependencies": {
    "@tenpamk2/sd-tag-defines": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "commander": "^12.0.0",
    "esbuild": "^0.21.4",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0",
    "yaml": "^2.4.2"
  }
}
