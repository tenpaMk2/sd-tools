{
  "name": "@tenpamk2/sd-random-generator",
  "version": "1.0.0",
  "main": "index.js",
  "author": "tenpaMk2 <wavyroom@gmail.com> (https://github.com/tenpaMk2)",
  "description": "ðŸŽ² Image generator with random characters, outfits, backgrounds and poses via the API of SD web UI (or SD Forge) ðŸŽ²",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "build:base": "npx esbuild --bundle --platform=node --out-extension:.js=.cjs src/index.mts --outdir=dist",
    "build:esbuild": "npm run build:base -- --minify",
    "build:declaration": "tsc",
    "build": "run-p build:esbuild build:declaration",
    "watch:esbuild": "npm run build:base -- --sourcemap --keep-names --watch",
    "watch:declaration": "tsc --watch",
    "watch": "run-p watch:esbuild watch:declaration",
    "clean": "rimraf dist",
    "exe": "node dist/index.cjs",
    "test": "vitest",
    "clean:sea": "rimraf dist-sea",
    "build:sea:config": "mkdirp dist-sea && node --experimental-sea-config sea-config.json",
    "build:sea:copy-node:mac": "cp $(command -v node) dist-sea/index && chmod a+w dist-sea/index",
    "build:sea:postject:mac": "npx postject dist-sea/index NODE_SEA_BLOB dist-sea/sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2 --macho-segment-name NODE_SEA",
    "build:sea:mac": "run-s clean:sea build:sea:config build:sea:copy-node:mac build:sea:postject:mac",
    "build:sea:copy-node:win": "node -e \"require('fs').copyFileSync(process.execPath, 'dist-sea/index.exe')\"",
    "build:sea:postject:win": "npx postject dist-sea/index.exe NODE_SEA_BLOB dist-sea/sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2",
    "build:sea:win": "run-s clean:sea build:sea:config build:sea:copy-node:win build:sea:postject:win"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "tenpaMk2"
  ],
  "dependencies": {
    "@tenpamk2/sd-tag-defines": "^1.0.0",
    "@types/lodash": "^4.17.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "lodash": "^4.17.21",
    "picocolors": "^1.0.1",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/node": "^20.12.7",
    "esbuild": "^0.21.4",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
